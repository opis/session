{
    "name": "opis/session",
    "description": "Opis Session is a session manager library with support for multiple backend stores",
    "keywords": ["opis", "session", "session store", "session management"],
    "homepage": "https://opis.io/session",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Marius Sarca",
            "email": "marius.sarca@gmail.com"
        },
        {
            "name": "Sorin Sarca",
            "email": "sarca_sorin@hotmail.com"
        }
    ],
    "require": {
        "php": "^7.4"
    },
    "require-dev": {
        "ext-json": "*",
        "phpunit/phpunit": "^9.0",
        "opis/http": "2020.x-dev",
        "opis/database": "^4.1"
    },
    "autoload": {
        "psr-4": {
            "Opis\\Session\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Opis\\Session\\Test\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "2020.x-dev"
        }
    }
}
